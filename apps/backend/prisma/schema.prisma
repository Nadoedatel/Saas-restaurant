generator client {
  provider = "prisma-client-js"
  // output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Booking {
  id        Int      @id @default(autoincrement())
  name      String
  phone     String
  time      DateTime
  tableNumber Int
  token       String   @default(uuid()) 
  isConfirmed Boolean  @default(false)
  createdAt DateTime @default(now())
}
